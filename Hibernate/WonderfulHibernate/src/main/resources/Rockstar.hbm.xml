<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

        <hibernate-mapping package="com.hibernate.model">
        <class name="Rockstar" table="Rockstar">
        
        <!-- THIS IS OPTIONAL -->
        <meta attribute="description">
        Describe some stuff or something
        </meta>
        
        <!-- PRIMARY KEY -->
        <id name="id" type="int" column="rock_id">
        <generator class="native"/>
        </id>
        
        <!-- PROPERTIES -->
        <property name="rockName" type="string">
        <column name="name" not-null="true" unique="true"/>
        </property>
        <property name="hairLength" column="hair_length" type="int"/>
        <property name="attitude" column="attitude" type="double"/>
        
        </class>
        
        
        </hibernate-mapping>