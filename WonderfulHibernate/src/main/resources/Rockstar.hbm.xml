<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0/EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.revature.model">
	<class name="Rockstar" table="Rock_star">

		<!-- PRIMARY KEY -->
		<id name="id" type="int" column="rock_id">
			<generator class="native" />
		</id>

		<!-- PROPERTIES -->
		<property name="name" type="string">
			<column name="name" not-null="true" unique="true" />
		</property>
		<property name="hairLength" column="hair_length" type="int" />
		<property name="instrument" column="instrument" type="string" />







	</class>

</hibernate-mapping>